{
    "name": "nevadskiy/laravel-position",
    "description": "The package allows you to arrange Laravel models in a given order.",
    "license": "MIT",
    "keywords": ["position", "sort", "order", "arrange", "model", "eloquent", "laravel"],
    "authors": [
        {
            "name": "Nevadskiy",
            "email": "nevadskiy@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "laravel/framework": "^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0",
        "orchestra/testbench": "^5.0|^6.0|^7.0",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Nevadskiy\\Position\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nevadskiy\\Position\\Tests\\": "tests/"
        }
    }
}
